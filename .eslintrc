{
  "parser": "babel-eslint",
  "extends": [
    "semistandard",
    "prettier"
  ],
  "env": {
    "jest": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "printWidth": 90
      }
    ]
  },
  "plugins": [
    "prettier",
    "standard",
    "import",
    "node",
    "promise"
  ]
}
